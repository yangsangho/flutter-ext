{
	"name": "flutter-ext",
	"displayName": "Flutter-Ext",
	"description": "reduce boilerplate",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.81.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/yangsangho/flutter-ext"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "flutter-ext.newView",
				"title": "FlutterExt: New View"
			},
			{
				"command": "flutter-ext.newModel",
				"title": "FlutterExt: New Model"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "flutter-ext.newView",
					"when": "explorerResourceIsFolder"
				},
				{
					"command": "flutter-ext.newModel",
					"when": "explorerResourceIsFolder"
				}
			]
		},
		"snippets": [
			{
				"language": "dart",
				"path": "./snippets/widget.json"
			},
			{
				"language": "dart",
				"path": "./snippets/functions.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/change-case": "^2.3.1",
		"@types/lodash": "^4.14.182",
		"@types/mkdirp": "^0.5.2",
		"@types/vscode": "^1.81.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vscode/test-electron": "^2.3.4",
		"eslint": "^8.47.0",
		"glob": "^10.3.3",
		"mocha": "^10.2.0",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"change-case": "^4.1.2",
		"lodash": "^4.17.19",
		"mkdirp": "^0.5.1"
	}
}